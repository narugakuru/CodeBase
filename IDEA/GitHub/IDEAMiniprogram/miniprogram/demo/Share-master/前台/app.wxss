
page {
  background-color: #ededed;
  background-image: linear-gradient(180deg, #a5caf1 0%,  #ffedf5 100%);
  width: 100%;
  margin: 0;
  padding: 0;
  z-index: -999;
  zoom: 1;
  /* background-color: #fff; */
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center 0;
}

/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  -webkit-overflow-scrolling: touch
}


button {
  font: inherit !important;

  margin: 0 !important;

  padding: 0 !important;

  background: none !important;

  line-height: unset !important;

  text-align: inherit !important;

  width: unset !important;

  color: inherit !important;

  position: unset !important;

  box-sizing: inherit !important;

  border: unset !important;

  min-height: unset !important;
}


.line {
  height: 1rpx;
  width: 100%;
  background: #999;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

/*背景层*/
#popLayer {
  background-color: rgba(220, 220, 220, 0.6);
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99999;
}
/*弹出层*/
#popBox {
  border-radius: 10rpx;
  padding: 20rpx;
  background-color: #ffffff;
  z-index: 11;
  width: 500rpx;
  height: 900rpx;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  z-index: 99999999;
}
